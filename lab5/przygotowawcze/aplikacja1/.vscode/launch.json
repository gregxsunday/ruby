    "version":"0.2.0",
    "configurations":[
       
       {
          "name":"Rails server",
          "type":"Ruby",
          "request":"launch",
          "cwd":"${workspaceFolder}",
          "program":"${workspaceFolder}/bin/rails",
          "args":[
             "server"
