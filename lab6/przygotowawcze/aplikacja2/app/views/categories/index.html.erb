<h1>Categories#index</h1>
<p>Find me in app/views/categories/index.html.erb</p>
<% Category.all.each do |cat| %>
    <%= cat.name %>:<br>
    <% Welcome.all.each do |wel|%>
        <% if wel.category_id == cat.id %>
            <%= wel.message %><br>
        <% end %>
    <% end %>
    <br><br>
<% end %>